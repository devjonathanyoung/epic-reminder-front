{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-shadow": [
      "error"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "curly": [
      "error"
    ],
    "brace-style": [
      "error"
    ],
    "max-len": [
      1,
      {
        "code": 180
      }
    ]
  }
}
